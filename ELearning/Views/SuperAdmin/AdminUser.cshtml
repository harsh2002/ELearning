@model List<ELearning.Model.UserModel>
@using ELearning.helper;

<section id="assessment">
    <div class="container">
        <div class="m-4"></div>
        <div class="col-lg-12 lead m-0">
            <div class="card">
                <div class="card-body">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Email</th>
                                <th>One time password</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @item.Name
                                    </td>
                                    <td>@item.Email</td>
                                    <td>
                                        @if(!string.IsNullOrEmpty(item.Password))
                                        {
                                            @item.Password.DecryptString()
                                        }
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</section>