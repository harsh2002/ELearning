@model List<ELearning.Model.InstituteModel>
<section id="activation">
    <div class="container">
        <div class="m-4"></div>
        <h3 class="text-uppercase text-secondary mb-0">Activation Code for User's</h3>
        <div class="row">
            <div class="col-lg-12 mx-auto lead m-0">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>ActivationCode</th>
                            <th>Date Created</th>
                            <th>Date Expired</th>
                            <th>Is Activated</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{ int i = 0;
                            foreach (var item in Model)
                            {
                                <tr>
                                    <td>@{i++;}@i</td>
                                    <td>@item.ActivationNumber</td>
                                    <td>@item.DateCreated</td>
                                    <td>@item.DateExpired</td>
                                    <td>@item.IsActivated</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>